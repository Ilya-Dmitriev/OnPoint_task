{
  "$schema": "https://json.schemastore.org/prettierrc.json",
  "printWidth": 100,
  "trailingComma": "all",
  "singleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "auto",
  "semi": true,
  "tabWidth": 2,
  "overrides": [
    {
      "files": ["*.scss", "*.css"],
      "options": {
        "singleQuote": false
      }
    }
  ]
}
